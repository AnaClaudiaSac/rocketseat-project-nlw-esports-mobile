{
  "expo": {
    "name": "Mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-nlw-esports.png",
    "scheme": "mobileapp",
    "splash": {
      "image": "./assets/images/logo-nlw-esports.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e004b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mobile.app",
      "newArchEnabled": true
    },
    "android": {
      "package": "com.mobile.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#121214"
      },
      "newArchEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
